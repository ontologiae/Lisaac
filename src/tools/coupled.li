Section Header

  + name        := COUPLED(E);

  - copyright   := "2003-2007 Benoit Sonntag";


  - author      := "Sonntag Benoit (bsonntag@loria.fr)";
  - comment     := "Couple object";

Section Inherit

  - parent_any:ANY := ANY;

Section Public

  + first :E;
  + second:E;

  - create elt1:E and elt2:E :SELF <-
  ( + result:SELF;

    result := clone;
    result.make elt1 and elt2;
    result
  );

  - make elt1:E and elt2:E <-
  (
    first  := elt1;
    second := elt2;
  );
