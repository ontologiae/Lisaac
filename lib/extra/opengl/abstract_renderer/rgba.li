Section Header

  + name     := RGBA;

  - comment  := "(r,g,b,a) color";

Section Inherit

  + parent_vector:Expanded VECTOR4(REAL_32);

  - parent_color:COLOR := COLOR;

Section Public

  - r:REAL_32 <- x;
  - g:REAL_32 <- y;
  - b:REAL_32 <- z;
  - a:REAL_32 <- w;

  - get_array:FAST_ARRAY(REAL_32) <- parent_vector.getv;
