Section Header

  + name        := TT_CMAP_SUBTABLE;
  - comment     :="Encoding Table for CMAP";

  - bibliography:="http://IsaacOS.com";
  - author      :="Boutet Jerome (boutet@loria.fr)";

Section Inherit

  - parent:OBJECT := OBJECT;

Section Public

  - create file:FILE :SELF <-
  ( + result:SELF;
    ? { file != NULL};

    result := clone;
    result.make file;
    result
  );

  - make file:FILE <- deferred;

  - print <- deferred;
