lisaac regexdna -O -i20
gcc -O3 -fomit-frame-pointer -o regexdna regexdna.c -lpcre
gcc -O3 -fomit-frame-pointer -o regexdna_c regexdna_c.c -lpcre
# g++ -O3 -lboost_regex regexdna_c.cc -o regexdna_c
echo Lisaac: 
time ./regexdna < input500000.txt > out_li.txt
echo C:
time ./regexdna_c < input500000.txt > out_c.txt
diff out_li.txt out_c.txt
