Section Header

  + name        := EXPR_BINARY;

  - author      := "Sonntag Benoit (bsonntag@loria.fr)";
  - comment     := "Binary Expression.";

Section Inherit

  + parent_expr:Expanded EXPR;

Section Public

  + left:EXPR;

  + right:EXPR;

  //
  // Constructor
  //

  - create l:EXPR and r:EXPR :SELF <-
  ( + result:SELF;

    result := clone;
    result.make l and r;
    result
  );

  - make l:EXPR and r:EXPR <-
  (
    left  := l;
    right := r;
  );
