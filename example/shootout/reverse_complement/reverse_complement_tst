lisaac reverse_complement -q -O -i20
gcc reverse_complement_c.c -o reverse_complement_c -O3 -fomit-frame-pointer -lm
echo Lisaac: 
time ./reverse_complement < revcomp-input.txt > out_li.txt
echo C:
time ./reverse_complement_c < revcomp-input.txt > out_c.txt
# time ./reverse_c 10 > out_c.txt
diff out_li.txt out_c.txt
