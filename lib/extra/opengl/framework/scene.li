Section Header

  + name     := SCENE;

  - comment  := "basic rendering scene";

Section Inherit

  - parent_object:OBJECT := OBJECT;

Section Public

  - renderer:RENDERER;

  - set_renderer r:RENDERER <-
  (
    renderer := r;
  );


  - initialize:BOOLEAN <- TRUE;

  - render t:REAL_32 <-
  (
    renderer.begin_frame;
    //------------

    //------------
    renderer.end_frame;
  );

  - release <-
  (
    // nothing
  );
