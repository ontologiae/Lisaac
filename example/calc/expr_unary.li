Section Header

  + name        := EXPR_UNARY;

  - author      := "Sonntag Benoit (bsonntag@loria.fr)";
  - comment     := "Unary Expression.";

Section Inherit

  + parent_expr:Expanded EXPR;

Section Public

  + expr:EXPR;

  //
  // Constructor
  //

  - create e:EXPR :SELF <-
  ( + result:SELF;

    result := clone;
    result.make e;
    result
  );

  - make e:EXPR <-
  (
    expr := e;
  );
