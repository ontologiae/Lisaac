Section Header

  + name     := MATRIX4(E);

  - comment  := "4x4 matrix";

Section Inherit

  + parent_matrix:Expanded ABSTRACT_MATRIX(E);

Section Public

  - lines:INTEGER <- 4;
  - columns:INTEGER <- 4;

  //
  // Creation.
  //

  - create:SELF <-
  ( + result:SELF;
    result := clone;
    result.make (4,4);
    result
  );

  - create_value val:INTEGER :SELF <-
  ( + result:SELF;
    result := clone;
    result.make (4,4);
    result.set_all_with val;
    result
  );

  //
  //
  //


